{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@database": ["database/db.ts"],
      "@components/*": [
        "components/*"
      ],
      "@context/*": [
        "context/*"
      ],
      "@util/*": [
        "utils/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@types/*": [
        "types/*"
      ],
    },    
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "strictNullChecks": false,
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "context/CalculateABY.js", "context/ethereum.js", "hooks/useFetch.js"  ],
  "exclude": [
    "node_modules"
  ]
}
